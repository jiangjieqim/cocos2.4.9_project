            "label": "copy build-templates",
            "type": "shell",
            "command": "echo off & cd../ && call skip build-templates",
            "problemMatcher": []
        },
        {
            "label": "构建 微信",
            "type": "shell",
            "command": "echo off & cd../ && call skip build_wx ${workspaceFolder}",
            "problemMatcher": []
        },
        {
            "label": "buildMetaClear",
            "type": "shell",
